        {
          "alias": "mobirise4-blocks/components",
          "_isSecondary": true,
          "typeCompatible": "!isAMP",
          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "& when (@bg-type = 'color')": {
              "background": "@bg-value"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)"
            },
            "& when (@scaleImg)": {
              ".team-card .image-wrap": {
                "overflow": "hidden",
                "img": {
                  "transition": "all .5s"
                },
                "&:hover": {
                  "img": {
                    "-webkit-transform": "scale3d(1.1, 1.1, 1.1)",
                    "transform": "scale3d(1.1, 1.1, 1.1)"
                  }
                }
              }
            },
            ".underline": {
              ".line": {
                "width": "2rem",
                "height": "1px",
                "background": "@underlineColor",
                "display": "inline-block"
              }
            },
            ".team-card": {
              "margin-bottom": "2rem"
            },
            ".card-wrap": {
              "background": "@cardBg",
              ".image-wrap img": {
                "width": "100%"
              },
              ".content-wrap": {
                "padding": "1.5rem",
                ".mbr-section-btn": {
                  "padding-top": "1rem",
                  ".btn": {
                    "margin": "0"
                  }
                }
              }
            },
            "@media (max-width: 767px)": {
              ".team-card.last-card": {
                "margin-bottom": "0"
              }
            },
            "@media (min-width: 768px) and (max-width: 991px)": {
              ".team-card.last-row-card": {
                "margin-bottom": "0"
              }
            },
